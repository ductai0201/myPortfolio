{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/blogs/(.*)",
      "destination": "/api/blogs/$1"
    },
    {
      "source": "/api/projects/(.*)",
      "destination": "/api/projects/$1"
    },
    {
      "source": "/api/tags/(.*)",
      "destination": "/api/tags/$1"
    },
    {
      "source": "/api/reviews/(.*)",
      "destination": "/api/reviews/$1"
    },
    {
      "source": "/(.*)",
      "destination": "index.js"
    }
  ]
}