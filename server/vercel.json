{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/blog/(.*)",
      "destination": "/api/blog/$1"
    },
    {
      "source": "/api/project/(.*)",
      "destination": "/api/project/$1"
    },
    {
      "source": "/api/tag/(.*)",
      "destination": "/api/tag/$1"
    },
    {
      "source": "/api/review/(.*)",
      "destination": "/api/review/$1"
    },
    {
      "source": "/(.*)",
      "destination": "index.js"
    }
  ]
}